[{
  "name": "Meta info",
  "code": "Got the meta stuff",
  "tests": [
    {
      "description": "&lt;meta&gt; has name set to udacity-grader",
      "definition": {
        "nodes": "meta",
        "limit": 1,
        "attribute": "name",
        "equals": "udacity-grader"
      }
    },
    {
      "description": "&lt;meta&gt; has a content tag with a link to json",
      "definition": {
        "nodes": "meta",
        "limit": 1,
        "attribute": "content",
        "hasSubstring": ".json"
      }
    }
  ]
}]